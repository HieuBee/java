package Buoi1TH1;

public class students {
	private String name;
	private int age;
	
	public students() {
		
	}
	public students(String ht, int t) {
		this.name = ht;
		this.age = t;
	}
	
	public void setName(String ht) {
		this.name = ht;
	}
	public String getName() {
		return name;
	}
	
	public void setAge(int t) {
		this.age = t;
	}
	public int getAge() {
		return age;
	}
	public void initData(String ten, int tuoi) {
		this.name = ten;
		this.age = tuoi;
	}
	public void displayInformation() {
		System.out.println("Ten sv: "+ name + ", tuoi: "+ age);
	}
}

